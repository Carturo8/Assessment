# Hostname of your PostgreSQL server (Supabase provides this)
DB_HOST=your-supabase-project.supabase.co

# Username used to connect to the PostgreSQL database (e.g., 'postgres.xxxxxxxx')
DB_USER=your_supabase_username

# Password used to connect to the PostgreSQL database
DB_PASSWORD=your_supabase_password

# Name of the PostgreSQL database you want to connect to (usually 'postgres' on Supabase)
DB_NAME=postgres

# Port used to connect to the PostgreSQL server (Supabase uses 6543 with SSL)
DB_PORT=6543

# Port for the Node.js server to listen on (default 3000)
PORT=3000
